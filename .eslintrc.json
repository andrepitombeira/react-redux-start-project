{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "globals": {
    "initialState": true,
    "describe": true,
    "it": true,
    "expect": true,
    "__DEV__": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-use-before-define": ["error", "nofunc"]
  }
}
